<div role='banner' class="header_dgeex">
  {{#if @blog.logo}}<a href="/"><img id="blog" class="logo_dgeex" src="{{@blog.logo}}" alt="{{@blog.title}}" /></a>{{/if}}
  <h1 class="blogtitle_dgeex">
    <a title="{{@blog.title}}" href="/">
      {{{@blog.title}}}
    </a>
  </h1>
  <p class="blogdescription_dgeex">
    <a title="{{@blog.title}}" href='{{@blog.url}}'>
      {{@blog.description}}
    </a>
  </p>
</div>

